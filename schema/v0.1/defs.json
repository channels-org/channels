{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/channels-org/channels/master/schema/v0.1/defs.json#",
  "definitions": {
    "seqId": {
      "$id": "#seqId",
      "title": "item/change ID",
      "description": "Item ID is 1-based sequential ID in the order of items/changes creation",
      "type": "integer",
      "minimum": 1
    },
    "timestamp": {
      "$id": "#timestamp",
      "type": "string",
      "format": "date-time"
    },
    "optionalUri": {
      "$id": "#optionalUri",
      "type": ["boolean", "string"],
      "format": "uri"
    }
  }
}
